{
  "buildCommand": "pip install -r requirements.txt",
  "env": {
    "FLASK_ENV": "production"
  },
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
